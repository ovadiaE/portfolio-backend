{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false
  },
  "include": ["src/**/*.ts", "index.ts", "models/**/*.ts"],
  "exclude": ["node_modules"],
  "no-unused-variable": [true, {"ignore-pattern": "^_"}]
}